{
  "name": "svelte-object-explorer",
  "version": "2.0.0",
  "svelte": "src/index.js",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "description": "A Svelte component to view objects similar to the console to easily see your state or stores",
  "homepage": "https://github.com/Swiftaff/svelte-object-explorer",
  "repository": {
    "type": "git",
    "url": "https://github.com/Swiftaff/svelte-object-explorer"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "svelte",
    "object",
    "explorer",
    "component",
    "devtool",
    "helper"
  ],
  "author": "swiftaff",
  "license": "MIT",
  "devDependencies": {
    "ava": "^3.15.0",
    "cypress": "^3.8.3",
    "esm": "^3.2.25",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.31.1",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-livereload": "^1.0.1",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^5.2.0",
    "sirv-cli": "^0.4.5",
    "start-server-and-test": "^1.10.8",
    "svelte": "^3.18.2",
    "svelte-loader": "^2.13.4",
    "svelte-spa-router": "^2.0.0"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "run-p start:dev build:dev",
    "test": "ava && npm run cy:run",
    "build:dev": "rollup -c && rollup -c rollup.config.example.js -w",
    "start:dev": "sirv public --dev",
    "npmpublish": "npm publish",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:ci": "start-server-and-test dev http://localhost:5000 cy:run",
    "unit-test": "ava --watch"
  },
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "svelte-icons": "^1.1.0"
  }
}