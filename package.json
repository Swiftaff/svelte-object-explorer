{
  "name": "svelte-object-explorer",
  "version": "2.0.0",
  "svelte": "src/Index.svelte",
  "main": "dist/index.js",
  "description": "A Svelte component to view objects similar to the console to easily see your state or stores",
  "homepage": "https://github.com/Swiftaff/svelte-object-explorer",
  "repository": {
    "type": "git",
    "url": "https://github.com/Swiftaff/svelte-object-explorer"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "svelte",
    "object",
    "explorer",
    "component",
    "devtool",
    "helper"
  ],
  "author": "swiftaff",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "ava": "^3.15.0",
    "cypress": "^7.6.0",
    "esm": "^3.2.25",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.52.7",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-iconify-svg": "^2.2.4",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^1.0.12",
    "start-server-and-test": "^1.12.5",
    "svelte": "^3.38.3"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "run-p start:dev build:dev build:svelte_component build:custom_element build:umd_module",
    "test": "ava && npm run cy:run",
    "build:dev": "rollup -c",
    "build:svelte_component": "rollup -c src/Examples/SvelteComponent/rollup.config.js -w",
    "build:custom_element": "rollup -c src/Examples/CustomElement/rollup.config.js -w",
    "build:umd_module": "rollup -c src/Examples/UMDmodule/rollup.config.js -w",
    "start:dev": "sirv public --dev",
    "npmpublish": "npm publish",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:ci": "start-server-and-test dev http://localhost:5000 cy:run",
    "unit-test": "ava --watch"
  },
  "files": [
    "src",
    "dist"
  ]
}